{
  "name": "@fralis/url-encoder",
  "version": "1.0.0",
  "description": "A tool written in JavaScript. It can be used for encoding and decoding a URL.",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha src/index.test.js",
    "generate-latest": "minify ./dist/index.js --out-file ./versions/url-encoder@latest.js --mangle.keepClassName",
    "generate-version": "cross-var minify ./dist/index.js --out-file ./versions/url-encoder@$npm_package_version.js --mangle.keepClassName",
    "babel": "babel src --out-dir dist",
    "copyfiles": "copyfiles README.md ./dist/ && copyfiles package.json ./dist/",
    "build": "npm test && npm run babel && npm run generate-latest && npm run generate-version && npm run copyfiles"
  },
  "keywords": [
    "url",
    "url encoder",
    "url decoder",
    "url encoding",
    "url decoding",
    "percent encoding"
  ],
  "author": "Francesco Lisandro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/udede/lis-url-encoder.git"
  },
  "bugs": {
    "url": "https://github.com/udede/lis-url-encoder/issues"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "homepage": "https://github.com/udede/lis-url-encoder#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-arrow-functions": "^7.13.0",
    "@babel/preset-env": "^7.14.2",
    "babel-minify": "^0.5.1",
    "copyfiles": "^2.4.1",
    "cross-var": "^1.1.0",
    "mocha": "^8.4.0",
    "prettier": "2.3.0",
    "request": "^2.88.2"
  }
}
